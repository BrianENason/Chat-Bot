<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
<category>
    <pattern>SOFTWARE</pattern>
    <template>
        <message>Are you more interested in Development or Maintenance of 
        Software Systems?</message> 
            <button>
                <text> DEVELOPMENT </text>
                <postback>SDEVELOPMENT</postback>
            </button>
            <button>
                <text>MAINTENANCE</text>
                <postback>SMAINTENANCE</postback>
            </button>
    </template>
</category>

<category>
    <pattern>HARDWARE</pattern>
